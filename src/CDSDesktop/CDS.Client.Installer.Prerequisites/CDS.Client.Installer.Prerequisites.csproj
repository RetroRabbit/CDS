cd $(SolutionDir)CDS.Web.DataService\bin &amp; $(ProjectDir)Requirements\Commands\Zip\Rar.exe a -r $(ProjectDir)Requirements\Prerequisites\CDS.Web.DataService.rar *.*
cmd /c del /Q $(ProjectDir)Requirements\Server\ 
cmd /c xcopy "$(ProjectDir)Requirements\Application\*.dll" "$(ProjectDir)Requirements\Server" /Y
